{
  "service": {
    "id": "gateway-1",
    "name": "gateway",
    "tags": ["api", "http", "v1"],
    "port": 8080,
    "address": "gateway",
    "meta": {
      "version": "1.0.0",
      "environment": "development"
    },
    "checks": [
      {
        "id": "gateway-http",
        "name": "Gateway HTTP Health Check",
        "http": "http://gateway:8080/health",
        "interval": "10s",
        "timeout": "2s"
      }
    ]
  }
}
