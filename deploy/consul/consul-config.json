{
  "datacenter": "dc1",
  "data_dir": "/consul/data",
  "log_level": "INFO",
  "server": true,
  "bootstrap_expect": 1,
  "ui": true,
  "client_addr": "0.0.0.0",
  "bind_addr": "0.0.0.0",
  "ports": {
    "http": 8500,
    "dns": 8600,
    "grpc": 8502
  },
  "connect": {
    "enabled": true
  },
  "enable_central_service_config": true,
  "telemetry": {
    "prometheus_retention_time": "60s",
    "disable_hostname": false
  }
}
